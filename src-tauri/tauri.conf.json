{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "singularity",
  "version": "1.0.0",
  "identifier": "com.misterprada.singularity",
  "mobile": {
    "android": {
      "features": {
        "webviewGPU": true
      }
    }
  },
  "app": {
    "windows": [
      {
        "title": "tauri_try",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "android",
      "msi"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 24,
      "targetSdkVersion": 33,
      "permissions": [
        "android.permission.INTERNET"
      ]
    }
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../dist"
  }
}